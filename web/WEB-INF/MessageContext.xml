<?xml version="1.0" encoding="UTF-8"?>
<!-- Spring配置文件的根元素，使用spring-beans-3.0.xsd语义约束 -->
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans" xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="alertMessage" class="cn.edu.tsinghua.sthu.message.AlertMessage" scope="prototype" autowire="byName" />
    <bean id="loginMessage" class="cn.edu.tsinghua.sthu.message.LoginMessage" scope="prototype" autowire="byName" />
    <bean id="showLoginPageMessage" class="cn.edu.tsinghua.sthu.message.ShowLoginPageMessage" scope="prototype" autowire="byName" />
    <bean id="homeMessage" class="cn.edu.tsinghua.sthu.message.HomeMessage" scope="prototype" autowire="byName" />
    <bean id="showApplyClassroomPageMessage" class="cn.edu.tsinghua.sthu.message.ShowApplyClassroomPageMessage" scope="prototype" autowire="byName" />
    <bean id="applyClassroomMessage" class="cn.edu.tsinghua.sthu.message.ApplyClassroomMessage" scope="prototype" autowire="byName" />
    <bean id="showApplyMessage" class="cn.edu.tsinghua.sthu.message.ShowApplyMessage" scope="prototype" autowire="byName" />
</beans>

